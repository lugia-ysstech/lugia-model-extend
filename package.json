{
  "name": "@lugiatest/lugia-template",
  "version": "1.0.0-alpha-10",
  "scripts": {
    "start": "lugia-scripts start",
    "build": "lugia-scripts build",
    "test": "lugia-scripts test -w",
    "clean": "npx shx rm -rf dist/ npm-shrinkwrap.all.json",
    "dist": "node scripts/create.js && npm run clean && npx cross-env BABEL_ENV=production gulp "
  },
  "dependencies": {
    "@lugia/lugiax": "^1.2.3",
    "date-fns": "^2.16.1",
    "format": "^0.2.2",
    "fs-extra": "^9.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-destructuring": "^7.9.5",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/types": "^7.12.12",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-react-flow-props-to-prop-types": "0.15.0",
    "babel-preset-react-app": "3.1.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-base64": "^0.1.3",
    "gulp-changed": "3",
    "gulp-newer": "1",
    "gulp-rename": "1.2.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.1",
    "gulp-watch": "4",
    "merge2": "^1.4.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@lugia/lugiax": "^1.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache License 2.0",
  "author": "ysstech",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "yarn run lint",
        "prettier --write",
        "git add"
      ],
      "*.{css,sass,scss,less,sss}": [
        "stylelint",
        "git add"
      ]
    },
    "ignore": [
      "*.min.{js,css}"
    ]
  },
  "main": "dist/modelInfos.js",
  "files": [
    "dist",
    "interface",
    "npm-shrinkwrap.json"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": []
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  }
}
